[PMU]
@ = 0x40000800, 0x800

SRC_CFG = 0x0010
> RCHF_SEL, 1, 1
= 48MHZ, 0
= 24MHZ, 1

> RCHF_EN, 0, 1
= DISABLE, 0
= ENABLE, 1

TRIM_POW0 = 0x0020
TRIM_POW1 = 0x0024
TRIM_POW2 = 0x0028
TRIM_POW3 = 0x002C

TRIM_RCHF = 0x0030
TRIM_RCLF = 0x0034
TRIM_OPA = 0x0038
TRIM_PLL = 0x003C

